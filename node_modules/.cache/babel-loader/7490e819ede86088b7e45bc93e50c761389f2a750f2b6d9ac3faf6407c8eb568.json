{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'Register',\n  data() {\n    return {\n      form: {\n        email: '',\n        password: '',\n        state: ''\n      },\n      error: '',\n      // Podrías poblar esto dinámicamente vía API\n      states: ['Aguascalientes', 'Baja California', 'Baja California Sur', 'Campeche', 'Chiapas', 'Chihuahua', 'Ciudad de México', 'Coahuila', 'Colima', 'Durango', 'Guanajuato', 'Guerrero', 'Hidalgo', 'Jalisco', 'México', 'Michoacán', 'Morelos', 'Nayarit', 'Nuevo León', 'Oaxaca', 'Puebla', 'Querétaro', 'Quintana Roo', 'San Luis Potosí', 'Sinaloa', 'Sonora', 'Tabasco', 'Tamaulipas', 'Tlaxcala', 'Veracruz', 'Yucatán', 'Zacatecas']\n    };\n  },\n  computed: {\n    isFormValid() {\n      return this.form.email && this.form.password && this.form.state;\n    }\n  },\n  methods: {\n    async onSubmit() {\n      this.error = '';\n      try {\n        await axios.post('/api/register', this.form);\n        // Tras registrarse, podrías redirigir al Login\n        this.$router.push({\n          name: 'Login'\n        });\n      } catch (err) {\n        this.error = err.response && err.response.data.message || 'Error al registrar, intenta de nuevo.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","email","password","state","error","states","computed","isFormValid","methods","onSubmit","post","$router","push","err","response","message"],"sources":["src/views/Register.vue"],"sourcesContent":["<!-- src/components/Register.vue -->\r\n<template>\r\n  <div class=\"d-flex justify-content-center align-items-center min-vh-100 bg-light\">\r\n    <div class=\"card w-100 mx-3\" style=\"max-width: 400px;\">\r\n      <div class=\"card-body\">\r\n        <h4 class=\"card-title text-center mb-4\">Registrarse</h4>\r\n\r\n        <div v-if=\"error\" class=\"alert alert-danger py-2\" role=\"alert\">\r\n          {{ error }}\r\n        </div>\r\n\r\n        <form @submit.prevent=\"onSubmit\">\r\n          <div class=\"mb-3\">\r\n            <label for=\"email\" class=\"form-label\">Correo electrónico</label>\r\n            <input\r\n              id=\"email\"\r\n              type=\"email\"\r\n              v-model.trim=\"form.email\"\r\n              class=\"form-control\"\r\n              placeholder=\"tú@correo.com\"\r\n              required\r\n            />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label for=\"password\" class=\"form-label\">Contraseña</label>\r\n            <input\r\n              id=\"password\"\r\n              type=\"password\"\r\n              v-model=\"form.password\"\r\n              class=\"form-control\"\r\n              placeholder=\"••••••••\"\r\n              required\r\n            />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label for=\"state\" class=\"form-label\">Estado de residencia</label>\r\n            <select\r\n              id=\"state\"\r\n              v-model=\"form.state\"\r\n              class=\"form-select\"\r\n              required\r\n            >\r\n              <option value=\"\" disabled>Selecciona un estado</option>\r\n              <option v-for=\"s in states\" :key=\"s\" :value=\"s\">{{ s }}</option>\r\n            </select>\r\n          </div>\r\n\r\n          <button\r\n            type=\"submit\"\r\n            class=\"btn btn-success w-100\"\r\n            :disabled=\"!isFormValid\"\r\n          >\r\n            Crear cuenta\r\n          </button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'Register',\r\n\r\n  data() {\r\n    return {\r\n      form: {\r\n        email: '',\r\n        password: '',\r\n        state: ''\r\n      },\r\n      error: '',\r\n      // Podrías poblar esto dinámicamente vía API\r\n      states: [\r\n        'Aguascalientes','Baja California','Baja California Sur','Campeche',\r\n        'Chiapas','Chihuahua','Ciudad de México','Coahuila','Colima',\r\n        'Durango','Guanajuato','Guerrero','Hidalgo','Jalisco','México',\r\n        'Michoacán','Morelos','Nayarit','Nuevo León','Oaxaca','Puebla',\r\n        'Querétaro','Quintana Roo','San Luis Potosí','Sinaloa','Sonora',\r\n        'Tabasco','Tamaulipas','Tlaxcala','Veracruz','Yucatán','Zacatecas'\r\n      ]\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    isFormValid() {\r\n      return (\r\n        this.form.email &&\r\n        this.form.password &&\r\n        this.form.state\r\n      )\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async onSubmit() {\r\n      this.error = ''\r\n      try {\r\n        await axios.post('/api/register', this.form)\r\n        // Tras registrarse, podrías redirigir al Login\r\n        this.$router.push({ name: 'Login' })\r\n      } catch (err) {\r\n        this.error =\r\n          (err.response && err.response.data.message) ||\r\n          'Error al registrar, intenta de nuevo.'\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.bg-light { background-color: #f8f9fa !important; }\r\n.card-body { display: flex; flex-direction: column; }\r\n</style>\r\n"],"mappings":";AA+DA,OAAAA,KAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,IAAA;QACAC,KAAA;QACAC,QAAA;QACAC,KAAA;MACA;MACAC,KAAA;MACA;MACAC,MAAA,GACA,wEACA,kEACA,qEACA,qEACA,qEACA;IAEA;EACA;EAEAC,QAAA;IACAC,YAAA;MACA,OACA,KAAAP,IAAA,CAAAC,KAAA,IACA,KAAAD,IAAA,CAAAE,QAAA,IACA,KAAAF,IAAA,CAAAG,KAAA;IAEA;EACA;EAEAK,OAAA;IACA,MAAAC,SAAA;MACA,KAAAL,KAAA;MACA;QACA,MAAAP,KAAA,CAAAa,IAAA,uBAAAV,IAAA;QACA;QACA,KAAAW,OAAA,CAAAC,IAAA;UAAAd,IAAA;QAAA;MACA,SAAAe,GAAA;QACA,KAAAT,KAAA,GACAS,GAAA,CAAAC,QAAA,IAAAD,GAAA,CAAAC,QAAA,CAAAf,IAAA,CAAAgB,OAAA,IACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}